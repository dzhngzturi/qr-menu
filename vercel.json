{
  "redirects": [
    {
      "source": "/menu/avva",
      "has": [{ "type": "host", "value": "qr-menu-sepia.vercel.app" }],
      "destination": "https://avvakaffe.com",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/", "has": [{ "type": "host", "value": "avvakaffe.com" }], "destination": "/menu/avva" },
    { "source": "/", "has": [{ "type": "host", "value": "www.avvakaffe.com" }], "destination": "/menu/avva" },

    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
