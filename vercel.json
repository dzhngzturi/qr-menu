{
  "redirects": [
    {
      "source": "/menu/avva",
      "has": [{ "type": "host", "value": "qr-menu-sepia.vercel.app" }],
      "destination": "https://avvakafe.com",
      "permanent": true
    },
    {
      "source": "/menu/avva/",
      "has": [{ "type": "host", "value": "qr-menu-sepia.vercel.app" }],
      "destination": "https://avvakafe.com",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/", "has": [{ "type": "host", "value": "avvakafe.com" }], "destination": "/menu/avva?tab=food" },
    { "source": "/", "has": [{ "type": "host", "value": "www.avvakafe.com" }], "destination": "/menu/avva?tab=food" },

    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
